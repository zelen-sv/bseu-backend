<div class="row">
  <div class="col-md-8">
    <div class="card">
      <div class="card-header">
        <strong>Просмотр факультета</strong>
        <%= link_to edit_admin_faculty_path(@faculty), class: "float-right" do %>
          <i class="fa fa-edit"></i>
        <% end %>
      </div>
      <div class="card-body">
        <%= render 'faculty_form', { faculty: @faculty, disabled: true } %>
      </div>
    </div>
  </div>
</div>


