<tr>
  <td><%= specialty.title %></td>
  <td><%= specialty.faculty.title %></td>
  <td><%= education_program_text(specialty.education_program) %></td>
  <td><%= education_form_text(specialty.education_form) %></td>
  <td class="table-actions-td">
    <%= link_to duplicate_admin_specialty_path(specialty), method: "post", class: "btn btn-warning" do %>
      <i class="fa fa-copy"></i>
    <% end %>
    <%= link_to admin_specialty_path(specialty), class: "btn btn-success" do %>
      <i class="fa fa-search-plus"></i>
    <% end %>
    <%= link_to edit_admin_specialty_path(specialty), class: "btn btn-info" do %>
      <i class="fa fa-edit"></i>
    <% end %>
    <%= link_to admin_specialty_path(specialty), method: 'delete', class: "btn btn-danger", data: {confirm: 'Вы уверены?'} do %>
      <i class="fa fa-trash-o"></i>
    <% end %>
  </td>
</tr>
